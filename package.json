{
  "name": "dashboard-mailbox-interface",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run generate-icons && vite",
    "build": "npm run generate-icons && tsc -b && vite build",
    "generate-icons": "node scripts/generate-icons.js",
    "watch-icons": "onchange \"src/icons/icons.ts\" -- npm run generate-icons",
    "watch-theme": "onchange \"src/theme.ts\" -- npx @chakra-ui/cli typegen ./src/theme.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.26.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "formik": "^2.4.6",
    "moment": "^2.30.1",
    "react": "^19.1.1",
    "react-data-table-component": "^7.7.0",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router": "^7.8.2",
    "styled-components": "^6.1.19",
    "uuid": "^12.0.0",
    "yup": "^1.7.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "onchange": "^7.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
